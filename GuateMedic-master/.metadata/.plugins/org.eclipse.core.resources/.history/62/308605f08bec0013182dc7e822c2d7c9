package com.example.guatemedic;

public class FamilyVisit {
	//family_id and visit_date form unique key
	private String mFamily_id;
	private String mVisit_date; 
	
	private String mFather_lives_with;
	private int mNum_pregnancies_mother;
	private int mNum_babies_alive;
	private int mNum_babies_dead;
	private String mHow_died; //Is this just text?
	private String mNum_children_under_5;
	private String mNum_people_in_household;
	private String mFathers_job;
	private String mIgss;
	
	public FamilyVisit() {
		
	}

	public String getFamily_id() {
		return mFamily_id;
	}

	public void setFamily_id(String family_id) {
		mFamily_id = family_id;
	}

	public String getVisit_date() {
		return mVisit_date;
	}

	public void setVisit_date(String visit_date) {
		mVisit_date = visit_date;
	}

	public String isFather_lives_with() {
		return mFather_lives_with;
	}

	public void setFather_lives_with(String father_lives_with) {
		mFather_lives_with = father_lives_with;
	}

	public int getNum_pregnancies_mother() {
		return mNum_pregnancies_mother;
	}

	public void setNum_pregnancies_mother(int num_pregnancies_mother) {
		mNum_pregnancies_mother = num_pregnancies_mother;
	}

	public int getNum_babies_alive() {
		return mNum_babies_alive;
	}

	public void setNum_babies_alive(int num_babies_alive) {
		mNum_babies_alive = num_babies_alive;
	}

	public int getNum_babies_dead() {
		return mNum_babies_dead;
	}

	public void setNum_babies_dead(int num_babies_dead) {
		mNum_babies_dead = num_babies_dead;
	}

	public String getHow_died() {
		return mHow_died;
	}

	public void setHow_died(String how_died) {
		mHow_died = how_died;
	}

	public String getNum_children_under_5() {
		return mNum_children_under_5;
	}

	public void setNum_children_under_5(String num_children_under_5) {
		mNum_children_under_5 = num_children_under_5;
	}

	public String getNum_people_in_household() {
		return mNum_people_in_household;
	}

	public void setNum_people_in_household(String num_people_in_household) {
		mNum_people_in_household = num_people_in_household;
	}

	public String getFathers_job() {
		return mFathers_job;
	}

	public void setFathers_job(String fathers_job) {
		mFathers_job = fathers_job;
	}

	public String getIgss() {
		return mIgss;
	}

	public void setIgss(String igss) {
		mIgss = igss;
	}

}
